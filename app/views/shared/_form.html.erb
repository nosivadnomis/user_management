<%= form_for @user do |f| %>
  <%= f.label :first_name, 'first name goes here:' %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name, 'your last name:' %>
  <%= f.text_field :last_name %>

  <%= f.label :email, 'email:' %>
  <%= f.text_field :email %>

  <%= f.label :password, 'password:' %>
  <%= f.password_field :password %>

  <%= f.submit %>
<% end %>